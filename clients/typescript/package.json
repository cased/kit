{
  "name": "@runcased/kit",
  "version": "0.1.0",
  "description": "TypeScript wrapper for Cased Kit CLI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepare": "npm run build",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test:int": "npm run build && jest --runTestsByPath src/__tests__/integration.test.ts"
  },
  "keywords": [
    "cased",
    "kit",
    "code-analysis",
    "semantic-search",
    "pr-review"
  ],
  "author": "Cased",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "prettier": "^3.2.0"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
